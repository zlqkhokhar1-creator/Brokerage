#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/dist/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/dist/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules:/Users/maria/Brokerage/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../.pnpm/ts-node@10.9.2_@types+node@24.3.1_typescript@5.9.2/node_modules/ts-node/dist/bin-script-deprecated.js" "$@"
fi
