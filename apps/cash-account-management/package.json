{
  "name": "cash-account-management",
  "version": "1.0.0",
  "description": "Comprehensive Cash Account Management System for Pakistani and International Markets with Multi-Payment Gateway Support",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "migrate": "node db/migrate.js",
    "migrate:up": "node db/migrate.js up",
    "migrate:down": "node db/migrate.js down",
    "seed": "node db/seed.js",
    "docker:build": "docker build -t cash-account-management .",
    "docker:run": "docker run -p 3007:3007 cash-account-management",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:3007/health || exit 1",
    "docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o swagger.json",
    "docs:serve": "swagger-ui-serve swagger.json"
  },
  "keywords": [
    "cash-account",
    "deposit",
    "withdrawal",
    "raast",
    "payment-gateway",
    "multi-currency",
    "fintech",
    "brokerage",
    "pakistani-market",
    "international"
