{
  "name": "mutual-fund-management",
  "version": "1.0.0",
  "description": "Comprehensive Mutual Fund Management System for Invest Pro Trading Platform",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:unit": "jest --selectProjects unit",
    "test:integration": "jest --selectProjects integration",
    "test:e2e": "jest --selectProjects e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "migrate": "node db/migrate.js",
    "migrate:up": "node db/migrate.js up",
    "migrate:down": "node db/migrate.js down",
    "seed": "node db/seed.js",
    "docker:build": "docker build -t mutual-fund-management .",
    "docker:run": "docker run -p 3006:3006 mutual-fund-management",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "health": "curl -f http://localhost:3006/health || exit 1",
    "docs": "swagger-jsdoc -d swaggerDef.js src/routes/*.js -o swagger.json",
    "docs:serve": "swagger-ui-serve swagger.json"
  },
  "keywords": [
    "mutual-funds",
    "investment",
    "portfolio-management",
    "trading",
    "fintech"
  ],
  "author": "Invest Pro Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "pg": "^8.11.3",
    "ioredis": "^5.3.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "axios": "^1.6.2",
    "node-cron": "^3.0.3",
    "socket.io": "^4.7.4",
    "nanoid": "^5.0.4",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "csv-parser": "^3.0.0",
    "xlsx": "^0.18.5",
    "pdf-lib": "^1.17.1",
    "nodemailer": "^6.9.7",
    "twilio": "^4.19.0",
    "compression": "^1.7.4",
    "express-mongo-sanitize": "^2.2.0",
    "express-slow-down": "^2.0.1",
    "express-brute": "^1.0.1",
    "express-brute-redis": "^0.0.1",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
